<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>ExperimentStorage</comment>

    <entry key="Version">1.0.0</entry>
    <entry key="NavMenuPhotoUrl">./resources/img/aust_nasa_downunder_arrow.jpg</entry>

    <!-- DBHost: Database driver -->
    <entry key="DBDriver">org.postgresql.Driver</entry>
    <!-- DBUrl: Database url which includes the database name -->
    <entry key="DBUrl">jdbc:postgresql://localhost:5432/ExperimentStorage</entry>
    <!-- DBPoolSize: Database connection pool size. Default is 1 if not specified -->
    <entry key="DBPoolSize">1</entry>
    <!-- DBUser: Database user name, privileges need to be granted for this user to use the database -->
    <entry key="DBUser"></entry>
    <!-- DBPassword: Database user's password -->
    <entry key="DBPassword"></entry>

</properties>
